    "@typescript-eslint/parser": "^6.0.0",
    "aws-sdk": "^2.792.0",
    "esbuild": "^0.18.11",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.6.1",
    "jest-mock": "^29.6.1",
    "prettier": "^2.5.1",
    "regenerator-runtime": "^0.13.9",
    "typescript": "^5.1.6"
  }
