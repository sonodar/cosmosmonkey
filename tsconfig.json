{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "outDir": "dist",
    "noImplicitReturns": true,
    "sourceMap": true,
    "strict": true,
    "baseUrl": ".",
    "rootDir": "./src",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "allowJs": true,
    "paths": {
      "models": ["./src/model"]
    }
  },
  "compileOnSave": true,
  "include": ["src"],
  "exclude": ["node_modules", "aws-sdk"]
}
